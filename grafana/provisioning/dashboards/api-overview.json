{
  "dashboard": {
    "title": "ZOGTest-Golang API Overview",
    "tags": ["opentelemetry", "golang", "api"],
    "timezone": "browser",
    "panels": [
      {
        "title": "Request Rate",
        "targets": [
          {
            "expr": "rate(http_server_requests_total[5m])",
            "legendFormat": "{{http_method}} {{http_route}}"
          }
        ],
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "title": "Response Time (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_server_request_duration_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_server_request_duration_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "title": "Error Rate",
        "targets": [
          {
            "expr": "rate(http_server_requests_total{http_status_code=~\"5..\"}[5m])",
            "legendFormat": "5xx errors"
          },
          {
            "expr": "rate(http_server_requests_total{http_status_code=~\"4..\"}[5m])",
            "legendFormat": "4xx errors"
          }
        ],
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "title": "In-Flight Requests",
        "targets": [
          {
            "expr": "http_server_requests_in_flight",
            "legendFormat": "Active requests"
          }
        ],
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      }
    ],
    "schemaVersion": 16,
    "version": 1
  }
}
